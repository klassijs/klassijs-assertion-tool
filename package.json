  },
  "license": "MIT",
  "keywords": [
    "chai",
    "expect-webdriverio",
    "javascript",
    "expect",
    "assert",
    "soft-assert",
    "testing",
    "assertions"
  ],
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/klassijs/klassijs-soft-assert.git"
  },
  "private": true,
  "dependencies": {
    "@wdio/globals": "^9.2.8",
